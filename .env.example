# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Yandex SpeechKit (for Russian AI calls)
YANDEX_API_KEY=your-yandex-api-key
YANDEX_FOLDER_ID=your-yandex-folder-id

# LLM Provider (choose one)
# Option 1: OpenAI
OPENAI_API_KEY=your-openai-api-key

# Option 2: Anthropic Claude
# ANTHROPIC_API_KEY=your-anthropic-api-key

# Option 3: Yandex GPT
# YANDEX_GPT_API_KEY=your-yandex-gpt-key

# Call Provider (e.g., Vapi)
VAPI_API_KEY=your-vapi-api-key
VAPI_PHONE_NUMBER_ID=your-vapi-phone-number-id

# Human Operator Transfer
OPERATOR_PHONE_NUMBER=+998901234567

# n8n Webhook URLs
N8N_WEBHOOK_BASE_URL=https://your-n8n-instance.com/webhook
N8N_SHEETS_WEBHOOK=https://your-n8n-instance.com/webhook/sheets-update
N8N_TELEGRAM_WEBHOOK=https://your-n8n-instance.com/webhook/telegram-alert
N8N_DAILY_SUMMARY_WEBHOOK=https://your-n8n-instance.com/webhook/daily-summary

# Security
WEBHOOK_SECRET=your-webhook-secret-for-validation

# Environment
NODE_ENV=development
